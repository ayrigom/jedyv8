	    "controller.animation.summon_shukaku": {
      "states": {
        "default": {
          "transitions": [ { "summon_shukaku": "query.is_using_item && query.get_equipped_item_name('main_hand') == 'summon_shukaku'" } ],
          "on_entry": [""]
        },
        "summon_shukaku": {
          "transitions": [ { "default": "query.is_using_item && query.get_equipped_item_name('main_hand') == 'summon_shukaku'" } ],
          "on_entry": [
          "/summon jedy:1shukaku ~ ~ ~",
          "/say §l§eShukaku spawnou!",
          "/replaceitem entity @p slot.weapon.mainhand 0 air"
          ]
        }
      }
    },
